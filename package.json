    "@datorama/akita": "^5.3.0",
    "@datorama/akita-ng-router-store": "^5.1.9",
    "@microsoft/signalr": "^3.1.3",
    "babel-polyfill": "^6.26.0",
    "core-js": "^2.6.11",
    "jquery": "^3.5.1",
    "jquery-ui-dist": "^1.12.1",
